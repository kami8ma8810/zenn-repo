{
  "name": "google-cropper-research",
  "version": "1.0.0",
  "description": "Googleプロフィール画像クロッパーの挙動調査ツール",
  "type": "module",
  "scripts": {
    "build": "esbuild src/main.ts --bundle --outfile=dist/cropper.js --format=esm",
    "build:watch": "npm run build -- --watch",
    "investigate": "tsx src/investigate-cropper.ts",
    "investigate:headless": "tsx src/investigate-cropper.ts --headless",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report"
  },
  "keywords": [
    "puppeteer",
    "image-cropper",
    "research",
    "google"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.48.0",
    "@types/node": "^22.0.0",
    "esbuild": "^0.27.0",
    "http-server": "^14.1.1",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "puppeteer": "^23.11.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2"
  }
}
